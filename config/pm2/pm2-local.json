{
  "apps" : [{
    "name"        : "am-checkout",
    "script"      : "/application/server/index.js",
		"watch": [
			"/application/server", 
			"/application/client"
		],
		"instances": 4,
		"max_memory_restart": "2G",
    "node_args": ["--debug=5860"],
    "ignoreWatch" : [
			"/application/.git",
			"/application/.tmp",
			"/application/.idea",
			"/application/config",
			"/application/node_modules", 
			"/application/build"
			]
  }]
}